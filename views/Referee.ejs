<%- include('./partials/Head.ejs') %>
    <%- include('./partials/Header.ejs') %>


        <div class="row">
            <div class="container">
                <h1 class="text-center">Nombre arbrito</h1>
                <form class="">
                    <div class="row">
                        <div class="form-group ">
                            <div class="col-12">
                                <select class="form-select form-select-sm form-select-category" aria-label=".form-select-sm example">
                                    <option selected>CATEGORIA</option>
                                    <option value="1">One</option>
                                    <option value="2">Two</option>
                                    <option value="3">Three</option>
                                </select>
                            </div>
                            <div class="row mt-6" >
                                <div class="col">
                                    <div>
                                        <label class="my-2" for="KATA">KATA</label>
                                        <br>
                                        <select class="form-select form-select-sm form-select-kata"
                                            aria-label=".form-select-sm example" id="KATA">
                                            <option selected>DETALLE</option>
                                            <option value="1">One</option>
                                            <option value="2">Two</option>
                                            <option value="3">Three</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col">
                                    <div >
                                        <select class="form-select form-select-sm form-select-nomEva"
                                            aria-label=".form-select-sm example">
                                            <option selected>NOM. EVALUAR</option>
                                            <option value="1">One</option>
                                            <option value="2">Two</option>
                                            <option value="3">Three</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col">
                                <div>
                                    <select
                                        class="form-select form-select-sm form-select-participants"
                                        aria-label=".form-select-sm example"
                                        >
                                        <option selected>PARTICIPANTES</option>
                                        <option value="1">One</option>
                                        <option value="2">Two</option>
                                        <option value="3">Three</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col border border-dark">
                            <h5 class="text-center">FISICO</h5>
                            <input class="pointF" type="number" class="form-control" value="00.0" step="0.1">
                        </div>
                        <div class="col border border-dark">
                            <h5 class="text-center">TECNICA</h5>
                            <input class="pointT" type="number" class="form-control" value="00.0" step="0.1">
                        </div>
                    </div>
                    <div class="col">
                        <div class="d-flex justify-content-center mt-4">
                            <button type="button" class="btn btn-success">
                                Finalizar
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>



        <%- include("./partials/Footer.ejs") %>